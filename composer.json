{
    "name": "spatie/laravel-model-meta",
    "description": "Get information about the models in your Laravel app",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-model-meta"
    ],
    "homepage": "https://github.com/spatie/laravel-model-meta",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.4",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/pest-plugin-snapshots": "^1.1",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ModelMeta\\": "src",
            "Spatie\\ModelMeta\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ModelMeta\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ModelMeta\\ModelMetaServiceProvider"
            ],
            "aliases": {
                "ModelMeta": "Spatie\\ModelMeta\\Facades\\ModelMeta"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
